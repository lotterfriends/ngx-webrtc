{
  "name": "video-chat",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --names=\"Express:video-chat-server,Angular:video-chat-ui\" \"npm run dev:server\" \"npm run start:ui\"",
    "start": "concurrently --names=\"Express:video-chat-server,Angular:video-chat-ui\" \"npm run start:server\" \"npm run start:ui\"",
    "start:server": "cd apps/server && npm start",
    "start:ui": "cd apps/ui && npm start",
    "install": "npm run install:ui && npm run install:server",
    "install:ui": "cd apps/ui && npm install",
    "install:server": "cd apps/server && npm install",
    "dev:server": "cd apps/server && npm run dev"
  },
